{
  "name": "verdaccio-audit",
  "version": "1.1.0",
  "description": "verdaccio middleware plugin to bypass npmjs audit",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/verdaccio/verdaccio-audit"
  },
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma none --write \"{src,test}/**/*.js\"",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "standard-version -a -s",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/ --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "keywords": [
    "verdaccio",
    "plugin",
    "middleware",
    "audit"
  ],
  "author": "Juan Picado <juanpicado19@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "4.16.4",
    "request": "2.88.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@types/express": "4.16.1",
    "@types/http-errors": "1.6.1",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/request": "2.48.1",
    "@typescript-eslint/eslint-plugin": "1.1.0",
    "@verdaccio/babel-preset": "0.0.3",
    "@verdaccio/types": "5.0.0-beta.1",
    "cross-env": "5.2.0",
    "eslint": "5.6.1",
    "eslint-config-google": "0.10.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-jest": "21.24.1",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.3.1",
    "jest": "24.0.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "standard-version": "4.4.0",
    "typescript": "3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && commitlint -e $GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=6.12.0",
    "npm": ">=3"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
}
