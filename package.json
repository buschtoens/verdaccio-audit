{
  "name": "verdaccio-audit",
  "version": "1.1.0",
  "description": "verdaccio middleware plugin to bypass npmjs audit",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/verdaccio/verdaccio-audit"
  },
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma none --write \"{src,test}/**/*.js\"",
    "lint": "npm run flow && eslint .",
    "flow": "flow",
    "prepublish": "npm run build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "standard-version -a -s",
    "build": "babel src/ --out-dir lib/ --copy-files --ignore ___tests___"
  },
  "keywords": [
    "verdaccio",
    "plugin",
    "middleware",
    "audit"
  ],
  "author": "Juan Picado <juanpicado19@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "4.16.4",
    "request": "2.88.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@verdaccio/types": "4.1.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "cross-env": "5.2.0",
    "eslint": "5.12.0",
    "eslint-config-google": "0.11.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-prettier": "3.0.1",
    "flow-bin": "0.89.0",
    "flow-runtime": "0.17.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "standard-version": "4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && commitlint -e $GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
}
